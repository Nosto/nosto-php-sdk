{
  "name": "nosto/php-sdk",
  "description": "PHP SDK for developing Nosto modules for e-commerce platforms",
  "license": "BSD-3-Clause",
  "require-dev": {
    "codeception/codeception": "@stable",
    "codeception/specify": "@stable",
    "etsy/phan": "0.8.0",
    "phpmd/phpmd": "^2.5",
    "squizlabs/php_codesniffer": "2.*",
    "sebastian/phpcpd": "*",
    "phpdocumentor/reflection-docblock": "3.0.0",
    "phing/phing": "2.*",
    "codeception/c3": "^2.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [
      "bootstrap.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "require": {
    "phpseclib/phpseclib": "2.0.4",
    "vlucas/phpdotenv": "2.4.0",
    "league/fractal": "0.15.0"
  },
  "suggested": {

  }
}
